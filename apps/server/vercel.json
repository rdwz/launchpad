{
  "framework": "astro",
  "devCommand": "cd ../.. && yarn dev",
  "buildCommand": "cd ../.. && yarn build",
  "installCommand": "yarn --cwd=../.. install",
  "ignoreCommand": "exit 1",
  "cleanUrls": true,
  "regions": ["fra1"],
  "trailingSlash": false,
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/fastify"
    }
  ]
}
